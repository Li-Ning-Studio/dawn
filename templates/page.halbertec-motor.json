sNextBtn = document.getElementById('shortsNextBtn');\n    let shortsCurrentIndex = 0;\n    \n    // Create dots\n    shortCards.forEach((_, index) => {\n        const dot = document.createElement('div');\n        dot.className = 'shorts-dot' + (index === 0 ? ' active' : '');\n        dot.addEventListener('click', () => {\n            scrollToShort(index);\n        });\n        shortsDotsContainer.appendChild(dot);\n    });\n    \n    function scrollToShort(index) {\n        // Handle looping\n        if (index >= shortCards.length) {\n            index = 0;\n        } else if (index < 0) {\n            index = shortCards.length - 1;\n        }\n        \n        const cardWidth = shortCards[0].offsetWidth;\n        const gap = window.innerWidth > 768 ? 24 : 16;\n        const scrollPosition = (cardWidth + gap) * index;\n        shortsWrapper.scrollTo({ left: scrollPosition, behavior: 'smooth' });\n        shortsCurrentIndex = index;\n        updateShortsDots();\n    }\n    \n    function updateShortsDots() {\n        document.querySelectorAll('.shorts-dot').forEach((dot, index) => {\n            dot.classList.toggle('active', index === shortsCurrentIndex);\n        });\n    }\n    \n    // Arrow button handlers\n    shortsPrevBtn.addEventListener('click', () => {\n        scrollToShort(shortsCurrentIndex - 1);\n    });\n    \n    shortsNextBtn.addEventListener('click', () => {\n        scrollToShort(shortsCurrentIndex + 1);\n    });\n    \n    // Update dots on scroll\n    let shortsScrollTimeout;\n    shortsWrapper.addEventListener('scroll', () => {\n        clearTimeout(shortsScrollTimeout);\n        shortsScrollTimeout = setTimeout(() => {\n            const cardWidth = shortCards[0].offsetWidth;\n            const gap = window.innerWidth > 768 ? 24 : 16;\n            const newIndex = Math.round(shortsWrapper.scrollLeft / (cardWidth + gap));\n            \n            if (newIndex !== shortsCurrentIndex && newIndex >= 0 && newIndex < shortCards.length) {\n                shortsCurrentIndex = newIndex;\n                updateShortsDots();\n            }\n        }, 100);\n    });\n</script>",
