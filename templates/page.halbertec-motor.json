        "custom_liquid": "<style>\n    .reviews-section {\n        width: 100%;\n        padding: 4rem 0;\n        overflow: hidden;\n    }\n    \n    .reviews-heading {\n        font-size: 5rem;\ncolor: #222;\n        text-align: center;\n        margin: 0 0.2rem 3rem;\n        font-weight: 700;\n        line-height: 3rem;\n        letter-spacing: -2px;\n    }\n    \n    .slider-container {\n        position: relative;\n        max-width: 100%;\n        margin: 0 auto;\n    }\n    \n    .videos-wrapper {\n        display: flex;\n        gap: 1.5rem;\n        overflow-x: auto;\n        scroll-behavior: smooth;\n        scrollbar-width: none;\n        -ms-overflow-style: none;\n        padding: 0 2rem 2rem;\n        scroll-snap-type: x mandatory;\n    }\n    \n    .videos-wrapper::-webkit-scrollbar {\n        display: none;\n    }\n    \n    .video-card {\n        min-width: 600px;\n        flex-shrink: 0;\n        position: relative;\n        padding-bottom: 33.75%; /* 16:9 aspect ratio for min-width 600px */\n        height: 0;\n        scroll-snap-align: center;\n        border-radius: 16px;\n        overflow: hidden;\n        box-shadow: 0 4px 20px rgba(0, 0, 0, 0.08);\n        transition: transform 0.3s, box-shadow 0.3s;\n    }\n    \n    .video-card:hover {\n        transform: translateY(-4px);\n        box-shadow: 0 8px 30px rgba(0, 0, 0, 0.12);\n    }\n    \n    .video-card iframe {\n        position: absolute;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n        border: none;\n    }\n    \n    .navigation-container {\n        display: flex;\n        justify-content: center;\n        align-items: center;\n        gap: 1.5rem;\n        margin-top: 1.5rem;\n    }\n    \n    .nav-arrow {\n        width: 48px;\n        height: 48px;\n        border-radius: 50%;\n        background: #f5f5f5;\n        border: none;\n        cursor: pointer;\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        font-size: 1.5rem;\n        transition: all 0.3s;\n        color: #333;\n    }\n    \n    .nav-arrow:hover {\n        background: #e0e0e0;\n        transform: scale(1.1);\n    }\n    \n    .nav-arrow:active {\n        transform: scale(0.95);\n    }\n    \n    .dots-container {\n        display: flex;\n        justify-content: center;\n        gap: 0.5rem;\n    }\n    \n    .dot {\n        width: 8px;\n        height: 8px;\n        border-radius: 50%;\n        background: #e0e0e0;\n        cursor: pointer;\n        transition: all 0.3s;\n    }\n    \n    .dot.active {\n        background: #000;\n        width: 24px;\n        border-radius: 4px;\n    }\n    \n    @media (max-width: 768px) {\n        .nav-arrow {\n            display: none;\n        }\n    }\n    \n    @media (max-width: 768px) {\n        .reviews-section {\n            padding: 3rem 0;\n        }\n        \n        .reviews-heading {\n            font-size: 3rem;\n            line-height: 2.5rem;\n            margin-bottom: 2rem;\n            padding: 0 1rem;\n        }\n        \n        .videos-wrapper {\n            padding: 0 1rem 1.5rem;\n            gap: 1rem;\n        }\n        \n        .video-card {\n            min-width: calc(100vw - 3rem);\n            padding-bottom: 56.25%; /* 16:9 aspect ratio */\n            border-radius: 12px;\n        }\n        \n        .navigation-container {\n            padding: 0 1rem;\n        }\n        \n        .dot {\n            width: 6px;\n            height: 6px;\n        }\n        \n        .dot.active {\n            width: 20px;\n        }\n    }\n</style>\n\n<div class=\"reviews-section\">\n    <h2 class=\"reviews-heading\">See What Experts Are Saying</h2>\n    \n    <div class=\"slider-container\">\n        <div class=\"videos-wrapper\" id=\"videosWrapper\">\n            <div class=\"video-card\">\n                <iframe src=\"https://www.youtube.com/embed/0JFZENnMmNw\" allowfullscreen></iframe>\n            </div>\n            <div class=\"video-card\">\n                <iframe src=\"https://www.youtube.com/embed/xYFebYrtuLU\" allowfullscreen></iframe>\n            </div>\n            <div class=\"video-card\">\n                <iframe src=\"https://www.youtube.com/embed/q0cnXenKXXc\" allowfullscreen></iframe>\n            </div>\n        </div>\n    </div>\n</div>\n\n<div class=\"navigation-container\">\n    <button class=\"nav-arrow\" id=\"prevBtn\">‹</button>\n    <div class=\"dots-container\" id=\"dotsContainer\"></div>\n    <button class=\"nav-arrow\" id=\"nextBtn\">›</button>\n</div>\n\n<script>\n    const wrapper = document.getElementById('videosWrapper');\n    const dotsContainer = document.getElementById('dotsContainer');\n    const videoCards = document.querySelectorAll('.video-card');\n    const prevBtn = document.getElementById('prevBtn');\n    const nextBtn = document.getElementById('nextBtn');\n    let currentIndex = 0;\n    let autoScrollInterval;\n    \n    // Create dots\n    videoCards.forEach((_, index) => {\n        const dot = document.createElement('div');\n        dot.className = 'dot' + (index === 0 ? ' active' : '');\n        dot.addEventListener('click', () => {\n            scrollToVideo(index);\n            resetAutoScroll();\n        });\n        dotsContainer.appendChild(dot);\n    });\n    \n    function scrollToVideo(index) {\n        // Handle looping\n        if (index >= videoCards.length) {\n            index = 0;\n        } else if (index < 0) {\n            index = videoCards.length - 1;\n        }\n        \n        const cardWidth = videoCards[0].offsetWidth;\n        const gap = window.innerWidth > 768 ? 24 : 16;\n        const scrollPosition = (cardWidth + gap) * index;\n        wrapper.scrollTo({ left: scrollPosition, behavior: 'smooth' });\n        currentIndex = index;\n        updateDots();\n    }\n    \n    function updateDots() {\n        document.querySelectorAll('.dot').forEach((dot, index) => {\n            dot.classList.toggle('active', index === currentIndex);\n        });\n    }\n    \n    // Arrow button handlers\n    prevBtn.addEventListener('click', () => {\n        scrollToVideo(currentIndex - 1);\n        resetAutoScroll();\n    });\n    \n    nextBtn.addEventListener('click', () => {\n        scrollToVideo(currentIndex + 1);\n        resetAutoScroll();\n    });\n    \n    // Update dots on scroll\n    let scrollTimeout;\n    wrapper.addEventListener('scroll', () => {\n        clearTimeout(scrollTimeout);\n        scrollTimeout = setTimeout(() => {\n            const cardWidth = videoCards[0].offsetWidth;\n            const gap = window.innerWidth > 768 ? 24 : 16;\n            const newIndex = Math.round(wrapper.scrollLeft / (cardWidth + gap));\n            \n            if (newIndex !== currentIndex && newIndex >= 0 && newIndex < videoCards.length) {\n                currentIndex = newIndex;\n                updateDots();\n            }\n        }, 100);\n    });\n    \n    // Auto-scroll functionality\n    function startAutoScroll() {\n        autoScrollInterval = setInterval(() => {\n            scrollToVideo(currentIndex + 1);\n        }, 5000); // Auto-scroll every 5 seconds\n    }\n    \n    function resetAutoScroll() {\n        clearInterval(autoScrollInterval);\n        startAutoScroll();\n    }\n    \n    // Pause auto-scroll on hover\n    wrapper.addEventListener('mouseenter', () => {\n        clearInterval(autoScrollInterval);\n    });\n    \n    wrapper.addEventListener('mouseleave', () => {\n        startAutoScroll();\n    });\n    \n    // Start auto-scrolling\n    startAutoScroll();\n</script>",
