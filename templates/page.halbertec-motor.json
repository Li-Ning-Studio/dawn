lectorAll('.dot').forEach((dot, i) => {\n        dot.classList.toggle('active', i === currentIndex);\n      });\n    }\n\n    prevBtn.addEventListener('click', () => {\n      scrollToVideo(currentIndex - 1);\n      resetAutoScroll();\n    });\n\n    nextBtn.addEventListener('click', () => {\n      scrollToVideo(currentIndex + 1);\n      resetAutoScroll();\n    });\n\n    let scrollTimeout;\n    wrapper.addEventListener('scroll', () => {\n      clearTimeout(scrollTimeout);\n      scrollTimeout = setTimeout(() => {\n        const cardWidth = videoCards[0].offsetWidth;\n        const gap = window.innerWidth > 768 ? 24 : 16;\n        const newIndex = Math.round(wrapper.scrollLeft / (cardWidth + gap));\n        if (newIndex !== currentIndex && newIndex >= 0 && newIndex < videoCards.length) {\n          currentIndex = newIndex;\n          updateDots();\n        }\n      }, 100);\n    });\n\n    function startAutoScroll() {\n      autoScrollInterval = setInterval(() => {\n        scrollToVideo(currentIndex + 1);\n      }, 5000);\n    }\n\n    function resetAutoScroll() {\n      clearInterval(autoScrollInterval);\n      startAutoScroll();\n    }\n\n    wrapper.addEventListener('mouseenter', () => clearInterval(autoScrollInterval));\n    wrapper.addEventListener('mouseleave', () => startAutoScroll());\n\n    startAutoScroll();\n  </script>\n</body>\n</html>",
