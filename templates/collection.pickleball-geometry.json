e: 1rem; color: var(--pg-text);\n    padding: 14px; background: linear-gradient(135deg, rgba(248,250,252,.8) 0%, rgba(241,245,249,.6) 100%);\n    border-radius: 12px; border: 1px solid rgba(148,163,184,.2); text-align: center; font-weight: 500; line-height: 1.4;\n  }\n  .pgmini__quick strong { color: var(--pg-em); font-weight: 700; }\n  .pgmini__quick span { font-weight: 800; color: var(--pg-active); position: relative; }\n\n  /* Responsive layout only inside .pgmini */\n  @media (min-width: 990px) {\n    .pgmini { \n      grid-template-columns: minmax(300px, 360px) 1fr;\n      grid-template-rows: auto auto 1fr auto;\n      align-items: start; gap: 24px; padding: 24px;\n    }\n    .pgmini__media { grid-column: 1; grid-row: 1 / 3; margin: 0; max-width: none; }\n    .pgmini__tabs  { grid-column: 2; grid-row: 1; width: 100%; margin: 0; }\n    .pgmini__content { grid-column: 2; grid-row: 2; }\n    .pgmini__tabs label { padding: 12px 18px; font-size: 1rem; }\n    .pgmini__panel { padding: 18px; }\n    .pgmini__title { font-size: 1.35rem; margin-bottom: 10px; }\n    .pgmini__one { font-size: 1.08rem; margin-bottom: 16px; }\n    .pgmini__list li { margin: 8px 0; font-size: 1.02rem; }\n    .pgmini__quick { grid-column: 1 / -1; grid-row: 3; margin-top: 12px; padding: 16px; font-size: 1.05rem; }\n  }\n\n  @media (max-width: 480px) {\n    .pgmini { padding: 16px; gap: 14px; border-radius: 16px; }\n    .pgmini__media img { max-height: 240px; width: auto; margin: 0 auto; }\n    .pgmini__tabs { gap: 8px; }\n    .pgmini__tabs label { padding: 10px 12px; font-size: .9rem; }\n    .pgmini__panel { padding: 16px; }\n    .pgmini__title { font-size: 1.125rem; }\n    .pgmini__one { font-size: 1rem; }\n    .pgmini__quick { padding: 14px; font-size: .95rem; }\n  }\n</style>",
