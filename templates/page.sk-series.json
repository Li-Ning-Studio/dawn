        "custom_liquid": "<div class=\"hndrd-product-section\"> \n\n  <!-- TEXT LEFT -->\n  <div class=\"hndrd-text-side\">\n    <div class=\"hndrd-text-inner\">\n      <img \n        class=\"hndrd-logo\"\n        src=\"https://cdn.shopify.com/s/files/1/0933/9378/1029/files/Logo_Attack.png?v=1764245037\"\n        alt=\"Logo\">\n\n      <p class=\"hndrd-subtitle\">\n        This is one of badminton's fiercest attackers, a racket built to play bold.\n      </p>\n\n      <a href=\"https://in.hndrd.co/products/nitrix-attack-4u\" class=\"hndrd-cta-btn\">\n        Shop Thunder\n      </a>\n\n      <div class=\"hndrd-hint\">\n        <span>Tap the image to reveal more</span>\n        <svg class=\"hndrd-arrow hndrd-arrow-right\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n          <path d=\"M5 12H19M19 12L12 5M19 12L12 19\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n        </svg>\n        <svg class=\"hndrd-arrow hndrd-arrow-down\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n          <path d=\"M12 5V19M12 19L5 12M12 19L19 12\" stroke=\"currentColor\" stroke-width=\"2\" stroke-linecap=\"round\" stroke-linejoin=\"round\"/>\n        </svg>\n      </div>\n    </div>\n  </div>\n\n  <!-- IMAGE RIGHT -->\n  <div class=\"hndrd-image-side\">\n    <div class=\"hndrd-flip-card hndrd-teaser hndrd-show-back\" onclick=\"this.classList.toggle('hndrd-flipped')\">\n      <div class=\"hndrd-flip-front\">\n        <img \n          src=\"https://cdn.shopify.com/s/files/1/0933/9378/1029/files/Post_1_2482bbf3-b11f-4f5e-acf2-406866a7c255.jpg?v=1764244924\" \n          alt=\"Front Image\">\n      </div>\n      <div class=\"hndrd-flip-back\">\n        <img \n          src=\"https://cdn.shopify.com/s/files/1/0933/9378/1029/files/1_b0b1a1a9-4bbd-4ba5-8337-2596cb05714c.jpg?v=1764245026\" \n          alt=\"Back Image\">\n      </div>\n    </div>\n  </div>\n\n</div>\n\n<style>\n  .hndrd-product-section {\n    display: flex;\n    width: 100%;\n    margin: 0;\n    padding: 0;\n  }\n\n  .hndrd-product-section .hndrd-image-side,\n  .hndrd-product-section .hndrd-text-side {\n    width: 50%;\n    margin: 0;\n    padding: 0;\n  }\n\n  /* FULL SCREEN IMAGE ON DESKTOP */\n  .hndrd-product-section .hndrd-image-side {\n    perspective: 1000px;\n    position: relative;\n    height: 100vh;\n    overflow: hidden;\n  }\n\n  .hndrd-product-section .hndrd-flip-card,\n  .hndrd-product-section .hndrd-flip-front,\n  .hndrd-product-section .hndrd-flip-back {\n    height: 100%;\n  }\n\n  .hndrd-product-section .hndrd-flip-card {\n    position: relative;\n    width: 100%;\n    transform-style: preserve-3d;\n    cursor: pointer;\n    transition: transform 0.6s;\n    transform: rotateY(0deg);\n  }\n\n  /* Initially show back image */\n  .hndrd-product-section .hndrd-flip-card.hndrd-show-back {\n    transform: rotateY(180deg);\n  }\n\n  /* When clicked/flipped, show front */\n  .hndrd-product-section .hndrd-flip-card.hndrd-flipped {\n    transform: rotateY(0deg);\n  }\n\n  /* Ensure back stays visible when not flipped */\n  .hndrd-product-section .hndrd-flip-card.hndrd-show-back:not(.hndrd-flipped) {\n    transform: rotateY(180deg);\n  }\n\n  .hndrd-product-section .hndrd-flip-front,\n  .hndrd-product-section .hndrd-flip-back {\n    width: 100%;\n    backface-visibility: hidden;\n    -webkit-backface-visibility: hidden;\n    position: relative;\n  }\n\n  .hndrd-product-section .hndrd-flip-back {\n    position: absolute;\n    top: 0;\n    left: 0;\n    transform: rotateY(180deg);\n  }\n\n  .hndrd-product-section .hndrd-flip-front img,\n  .hndrd-product-section .hndrd-flip-back img {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n    display: block;\n  }\n\n  /* TEXT SECTION */\n  .hndrd-product-section .hndrd-text-side {\n    background: transparent !important;\n    display: flex;\n    align-items: center;\n  }\n\n  .hndrd-product-section .hndrd-text-inner {\n    padding: 60px;\n    width: 100%;\n    color: #d6d6d6; \n  }\n\n  .hndrd-product-section .hndrd-logo {\n    width: 320px;\n    max-width: 100%;\n    margin-bottom: 20px;\n  }\n\n  .hndrd-product-section .hndrd-subtitle {\n    font-size: 20px;\n    line-height: 1.5;\n    margin-bottom: 30px;\n    color: #d6d6d6;\n  }\n\n  /* BUTTON */\n  .hndrd-product-section .hndrd-cta-btn {\n    display: inline-block;\n    padding: 14px 42px;\n    font-size: 16px;\n    font-weight: 700;\n    letter-spacing: 0.5px;\n    background: transparent;\n    border: 2px solid #a70000;\n    border-radius: 40px;\n    color: #a70000;\n    cursor: pointer;\n    text-decoration: none;\n    transition: background 0.2s ease, color 0.2s ease, border-color 0.2s ease;\n  }\n\n  .hndrd-product-section .hndrd-cta-btn:hover {\n    background: rgba(255, 42, 0, 0.15);\n    color: #ff2a00;\n    border-color: #ff2a00;\n  }\n\n  /* HINT TEXT WITH ARROW */\n  .hndrd-product-section .hndrd-hint {\n    display: flex;\n    align-items: center;\n    gap: 8px;\n    margin-top: 24px;\n    color: #999;\n    font-size: 14px;\n    opacity: 0.8;\n  }\n\n  .hndrd-product-section .hndrd-arrow {\n    color: #a70000;\n  }\n\n  /* Show right arrow on desktop, hide down arrow */\n  .hndrd-product-section .hndrd-arrow-right {\n    display: block;\n    animation: hndrd-bounce-right 2s ease-in-out infinite;\n  }\n\n  .hndrd-product-section .hndrd-arrow-down {\n    display: none;\n  }\n\n  @keyframes hndrd-bounce-right {\n    0%, 100% { transform: translateX(0); }\n    50% { transform: translateX(4px); }\n  }\n\n  @keyframes hndrd-bounce-down {\n    0%, 100% { transform: translateY(0); }\n    50% { transform: translateY(4px); }\n  }\n\n  /* SUBTLE TEASER ANIMATION FOR CLICKABLE CARD */\n  @keyframes hndrd-teaser-wiggle {\n    0% { transform: rotateY(180deg); }\n    50% { transform: rotateY(182deg); }\n    100% { transform: rotateY(180deg); }\n  }\n\n  .hndrd-product-section .hndrd-teaser.hndrd-show-back:not(.hndrd-flipped) {\n    animation: hndrd-teaser-wiggle 2s ease-in-out 3s infinite;\n    animation-play-state: running;\n  }\n\n  /* Stop animation once user interacts */\n  .hndrd-product-section .hndrd-teaser.hndrd-flipped {\n    animation: none !important;\n  }\n\n  /* MOBILE */\n  @media(max-width: 768px) {\n    .hndrd-product-section {\n      flex-direction: column;\n    }\n\n    .hndrd-product-section .hndrd-image-side {\n      height: auto;\n    }\n\n    .hndrd-product-section .hndrd-flip-front img,\n    .hndrd-product-section .hndrd-flip-back img {\n      height: auto;\n      object-fit: contain;\n    }\n\n    .hndrd-product-section .hndrd-text-side,\n    .hndrd-product-section .hndrd-image-side {\n      width: 100%;\n    }\n\n    .hndrd-product-section .hndrd-text-inner {\n      padding: 30px;\n      text-align: center;\n    }\n\n    .hndrd-product-section .hndrd-logo {\n      margin: 0 auto 15px auto;\n    }\n\n    .hndrd-product-section .hndrd-cta-btn {\n      margin: 0 auto;\n    }\n\n    .hndrd-product-section .hndrd-hint {\n      justify-content: center;\n    }\n\n    /* Show down arrow on mobile, hide right arrow */\n    .hndrd-product-section .hndrd-arrow-right {\n      display: none;\n    }\n\n    .hndrd-product-section .hndrd-arrow-down {\n      display: block;\n      animation: hndrd-bounce-down 2s ease-in-out infinite;\n    }\n  }\n</style>",
