      "settings": {
        "custom_liquid": "<style>\n  .badminton-section * {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n  }\n\n  .badminton-section .container {\n    max-width: 1200px;\n    margin: 0 auto;\n    padding: 2rem 1rem;\n  }\n\n  .badminton-section .products-row {\n    display: flex;\n    justify-content: space-between;\n  }\n\n  .badminton-section .product-column {\n    flex: 0 0 23%;\n    padding: 1.5rem;\n    text-align: center;\n    border-left: 1px solid #ccc;\n    margin-bottom: 2rem;\n  }\n\n  .badminton-section .product-column:first-child {\n    border-left: none;\n  }\n\n  .badminton-section .racket-img {\n    width: 40%;\n    height: auto;\n    margin-bottom: 0.5rem;\n  }\n\n  .badminton-section .product-title-text {\n    font-weight: 700;\n    font-size: 1.4rem;\n    margin-bottom: 0.75rem;\n    background-clip: text;\n    -webkit-background-clip: text;\n    color: transparent;\n  }\n\n  .badminton-section .venom-title {\n    background-image: linear-gradient(to right, #3bb537, #1b3822);\n  }\n\n  .badminton-section .volta-title {\n    background-image: linear-gradient(to right, #3ca6f3, #1b1e3f);\n  }\n\n  .badminton-section .flareon-title {\n    background-image: linear-gradient(to right, #f44336, #8e0e00);\n  }\n\n  .badminton-section .hyfonic-title {\n    background-image: linear-gradient(to right, #9f5cf7, #0c0624);\n  }\n\n  .badminton-section .attack-tag {\n    display: inline-block;\n    font-size: 0.75rem;\n    font-weight: 500;\n    background: rgba(0, 0, 0, 0.05);\n    padding: 0.35rem 0.75rem;\n    border-radius: 999px;\n    margin-bottom: 1rem;\n  }\n\n  .badminton-section .specs-section {\n    font-size: 1.1rem;\n    margin-top: 1rem;\n    text-align: center;\n  }\n\n  .badminton-section .spec-block {\n    margin-bottom: 1.1rem;\n  }\n\n  .badminton-section .spec-label {\n    font-weight: 600;\n    color: #1a1a1a;\n    margin-bottom: 0.2rem;\n  }\n\n  .badminton-section .spec-values {\n    color: #444;\n    margin-top: 0.2rem;\n  }\n\n  .badminton-section .feature-list {\n    list-style: none;\n    padding-left: 0;\n    margin: 0.25rem auto 0;\n    display: inline-block;\n    text-align: center;\n    line-height: 1.4;\n  }\n\n  .badminton-section .feature-list li {\n    margin: 0.2rem 0;\n  }\n\n  /* Mobile View - Swipable */\n  @media (max-width: 768px) {\n    .badminton-section .container {\n      padding: 1rem 0;\n      overflow: hidden;\n    }\n\n    .badminton-section .products-row {\n      display: flex;\n      overflow-x: auto;\n      scroll-snap-type: x mandatory;\n      -webkit-overflow-scrolling: touch;\n      scrollbar-width: none;\n      -ms-overflow-style: none;\n      gap: 1rem;\n      padding: 0 1rem;\n    }\n\n    .badminton-section .products-row::-webkit-scrollbar {\n      display: none;\n    }\n\n    .badminton-section .product-column {\n      flex: 0 0 calc(50% - 0.5rem);\n      min-width: calc(50% - 0.5rem);\n      border-left: 1px solid #ccc;\n      border-right: 1px solid #ccc;\n      border-radius: 12px;\n      padding: 1rem;\n      margin-bottom: 1rem;\n      scroll-snap-align: start;\n      background: #fafafa;\n    }\n\n    .badminton-section .product-column:first-child {\n      border-left: 1px solid #ccc;\n    }\n\n    .badminton-section .product-title-text {\n      font-size: 1.1rem;\n    }\n\n    .badminton-section .racket-img {\n      width: 90%;\n      margin: 0 auto 0.5rem;\n    }\n\n    .badminton-section .specs-section {\n      font-size: 0.8rem;\n    }\n\n    .badminton-section .spec-block {\n      margin-bottom: 0.8rem;\n    }\n\n    .badminton-section .attack-tag {\n      font-size: 0.7rem;\n      padding: 0.25rem 0.5rem;\n    }\n  }\n\n  /* Very small screens */\n  @media (max-width: 480px) {\n    .badminton-section .product-column {\n      flex: 0 0 calc(50% - 0.5rem);\n      min-width: calc(50% - 0.5rem);\n      padding: 0.8rem;\n    }\n\n    .badminton-section .product-title-text {\n      font-size: 1rem;\n    }\n\n    .badminton-section .specs-section {\n      font-size: 0.75rem;\n    }\n  }\n\n  /* Scroll indicator for mobile */\n  @media (max-width: 768px) {\n    .badminton-section .container::after {\n      content: \"← Swipe to see more →\";\n      display: block;\n      text-align: center;\n      color: #888;\n      font-size: 0.8rem;\n      margin-top: 1rem;\n      font-style: italic;\n    }\n  }\n</style>\n\n<div class=\"badminton-section\">\n  <div class=\"container\">\n    <div class=\"products-row\">\n\n      <!-- Nitrix 70 Venom -->\n      <div class=\"product-column\">\n        <img class=\"racket-img\" src=\"https://cdn.shopify.com/s/files/1/0933/9378/1029/files/Venom.png?v=1753182756\" alt=\"Nitrix 70 Venom Badminton Racket\" />\n        <div class=\"product-title-text venom-title\">NITRIX 70 VENOM</div>\n        <div class=\"attack-tag\">⚔️ Attack Oriented</div>\n        <div class=\"specs-section\">\n          <div class=\"spec-block\"><div class=\"spec-label\">Weight</div><div class=\"spec-values\">3U<br>4U</div></div>\n          <div class=\"spec-block\"><div class=\"spec-label\">Tension</div><div class=\"spec-values\">≤ 32 Lbs</div></div>\n          <div class=\"spec-block\"><div class=\"spec-label\">Shaft Diameter</div><div class=\"spec-values\">6.1mm</div></div>\n          <div class=\"spec-block\"><div class=\"spec-label\">Handle Size</div><div class=\"spec-values\">G6</div></div>\n          <div class=\"spec-block\"><div class=\"spec-label\">Player Level</div><div class=\"spec-values\">Professional</div></div>\n          <div class=\"spec-block\">\n            <div class=\"spec-label\">Key Technologies</div>\n            <ul class=\"feature-list\">\n              <li>T-Fuse</li>\n              <li>CNTRLfoam</li>\n              <li>M46x Carbon Fibre</li>\n              <li>Curve²</li>\n            </ul>\n          </div>\n        </div>\n      </div>\n\n      <!-- Nitrix 70 Volta -->\n      <div class=\"product-column\">\n        <img class=\"racket-img\" src=\"https://cdn.shopify.com/